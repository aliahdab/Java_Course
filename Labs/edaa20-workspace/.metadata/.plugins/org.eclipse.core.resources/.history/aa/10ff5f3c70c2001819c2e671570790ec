import se.lth.cs.pt.square.Square;
import se.lth.cs.pt.window.SimpleWindow;


public class test {
	public static void main(String[] args) {
		
		Square sq1 = new Square(20, 10, 30);
		SimpleWindow w = new SimpleWindow(600,600,"Square");
		sq1.move(20, 30);
		sq1.draw(w);
		Square sq2 = new Square(100,20,40);
		sq1 = sq2;
		sq1.draw(w);
System.out.println("Arian Ã¤r " + sq1.getArea());
	}
}