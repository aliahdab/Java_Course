public class Rational {
	private int num;
	private int den;
	
	public Rational() {
		this.num=0;
		this.den=0;
	}
	
	public void printRational() {
	System.out.print(this.num + "/");
	System.out.print(this.den);
	System.out.println();
	}
	
	public static void main(String[]args) {
		Rational r = new Rational();
		r.num=4;
		r.den=3;
		r.printRational();
	}
	
}